      <i-tab-bar-item key="competitor" icon="browse" current-icon="browse_fill" title="竞品对比"></i-tab-bar-item>
      <i-tab-bar-item key="scenes" icon="emoji" current-icon="emoji_fill" title="使用场景"></i-tab-bar-item>
      <i-tab-bar-item key="market" icon="service" current-icon="service_fill" title="市场分析"></i-tab-bar-item>
      <i-tab-bar-item key="contact" icon="mine" current-icon="mine_fill" title="联系方式"></i-tab-bar-item>
